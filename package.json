{
  "name": "funnel-editor",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "node server.js",
    "build": "react-scripts build && npm run build:server",
    "build:server": "mkdir -p build && cp server.js build/ && cp package.json build/ && cp package-lock.json build/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "react-router-dom": "^6.22.0",
    "firebase": "^10.12.3",
    "firebase-admin": "^12.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ajv": "6.12.6",
    "ajv-keywords": "3.5.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "gh-pages": "^6.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  }
}
